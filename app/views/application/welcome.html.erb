<h1>Welcome to Flicks</h1>

<% if @user %>
  <h1><img src="<%= @user.image %>"><%= @user.name %></h1>
  <p>Email: <%= @user.email %></p>
  <!--<h2>Facebook UID: <%= @user.uid %></h2> -->
  <% else %>
  <p>New User? <%= link_to "Sign Up", new_user_path %></p>
  <p> <%= link_to "Log in", login_path %></p>


<% end %>